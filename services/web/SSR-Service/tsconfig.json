{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "rootDir": "src",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "jsx": "react-jsx",
        "moduleResolution": "node10",
    },
    "include": [
        "src",
        "copySQL.js"
    ],
    "exclude": [
        "node_modules"
    ]
}